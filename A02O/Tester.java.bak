class Tester
{
	static void add()
	{
		int a=10;
		int b=20;
        int c=a+b;
		if((a*b)==c);
		{
			System.out.println((a+b)-(a-b));
		}
		else
		{
			System.out.println((a-b)+(a+b));
		}
}
        static void mul()
		int a=5;
		int b=4;
		int c=a*b;
		if ((a+b)==c));
	{
		System.out.println((a+b)+(a-b));
	}
	else
	{
		System.out.println((a-b)-(a+b));
	}
}
public static void main(String[]args)
{
		add();
		mul();
		}